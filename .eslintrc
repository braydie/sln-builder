{
  "rules": {
    "no-console": 0,
    "indent": 0,
    "quotes": [2, "single"],
    "semi": [2, "always"],
    "constructor-super": 1,
    "generator-star-spacing": 0,
    "no-this-before-super": 1,
    "no-var": 1,
    "object-shorthand": 0,
    "prefer-const": 1,
    "prefer-arrow-callback": "warn"
  },
  "env": {
    "es6": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "sourceType": "module"
  }
}
